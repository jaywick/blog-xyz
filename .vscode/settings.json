// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.vscode": true,
        "**/.vs": true,
        "**/bin": true,
        "**/obj": true,
        "*.sln": true,
        "*.njsproj": true,
        "LICENSE": true,
        "README.md": true,
        ".gitignore": true,
        "*.bat": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.js.map": true
    }
}